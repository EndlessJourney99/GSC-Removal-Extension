{
    "name": "Google Search Console AUTO Remove URLS",
    "description": "Auto remove url from GSC without copy and paste it manualy",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "https://www.example.com/",
        "https://example.com/"
    ],
    "action": {
        "default_popup": "index.html",
        "default_title": "Open the popup"
    }
}